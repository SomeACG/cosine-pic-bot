{
  "name": "cosine-pic-bot",
  "version": "0.1.0",
  "description": "发图的",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/app.ts --files",
    "db:init": "npx prisma migrate dev --name init",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:view": "npx prisma studio",
    "lint": "eslint src/**/*.ts --fix",
    "pm2": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:log": "pm2 log ecosystem.config.js",
    "test": "npx jest --runTestsByPath src/tests/twitter.test.ts",
    "db:convert": "node scripts/convertOldData.js && npx prisma generate",
    "change": "git cliff -o CHANGELOG.md"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
