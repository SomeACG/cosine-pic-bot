    "@prisma/client": "5.13.0",
    "aws-sdk": "^2.1535.0",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.3.1",
    "es-toolkit": "^1.32.0",
    "grammy": "^1.22.4",
    "mime-types": "^2.1.35",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "sqlite3": "^5.1.7",
    "tough-cookie": "^4.1.4",
    "ts-node": "^10.9.2",
